+// SPDX-License-Identifier: Unlicense OR MIT
+
+package explorer
+
+import (
+	"io"
+	"os"
+	"path/filepath"
+	"strings"
+	"unsafe"
+
+	"gioui.org/app"
+	"gioui.org/io/event"
