+// SPDX-License-Identifier: Unlicense OR MIT
+
+// The customdeco program demonstrates custom decorations
+// in Gio.
+package main
+
+import (
+	"fmt"
+	"image/color"
+	"log"
+	"os"
+
+	"gioui.org/app"
+	"gioui.org/io/system"
+	"gioui.org/layout"
+	"gioui.org/op"
+	"gioui.org/op/clip"
+	"gioui.org/op/paint"
+	"gioui.org/text"
