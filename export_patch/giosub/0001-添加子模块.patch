+func newNotifier() (Notifier, error) {
+	conn, err := dbus.SessionBus()
+	if err != nil {
+		return nil, fmt.Errorf("failed connecting to dbus: %w", err)
+	}
+	notifier, err := notify.New(conn)
+	if err != nil {
+		return nil, fmt.Errorf("failed creating notifier: %w", err)
+	}
+	return &dbusNotifier{
+		Notifier: notifier,
+	}, nil
