+// SPDX-License-Identifier: Unlicense OR MIT
+
+package main
+
+// A Gio program that displays Go contributors from GitHub. See https://gioui.org for more information.
+
+import (
+	"fmt"
+	"image"
+	"image/color"
+	"log"
+	"runtime"
+
+	"gioui.org/font"
+	"gioui.org/font/gofont"
+	"gioui.org/gesture"
+	"gioui.org/io/key"
+	"gioui.org/layout"
+	"gioui.org/op"
+	"gioui.org/op/clip"
+	"gioui.org/op/paint"
+	"gioui.org/text"
+	"gioui.org/unit"
