diff --git a/shader/gio/zcopy.vert.0.glsl100es b/shader/gio/zcopy.vert.0.glsl100es
new file mode 100644
index 00000000..8c2df103
--- /dev/null
+++ b/shader/gio/zcopy.vert.0.glsl100es
@@ -0,0 +1,35 @@
+#version 100
+
+struct m3x2
+{
+    vec3 r0;
+    vec3 r1;
+};
+
+struct Block
+{
+    vec2 scale;
+    vec2 pos;
+    vec2 uvScale;
+};
+
+uniform Block _block;
+
+varying vec2 vUV;
+attribute vec2 uv;
+attribute vec2 pos;
+
+vec3 transform3x2(m3x2 t, vec3 v)
+{
+    return vec3(dot(t.r0, v), dot(t.r1, v), dot(vec3(0.0, 0.0, 1.0), v));
+}
+
+void main()
+{
+    vUV = uv * _block.uvScale;
+    vec2 p = (pos * _block.scale) + _block.pos;
+    m3x2 param = m3x2(vec3(1.0, 0.0, 0.0), vec3(0.0, -1.0, 0.0));
+    vec3 param_1 = vec3(p, 0.0);
+    gl_Position = vec4(transform3x2(param, param_1), 1.0);
+}
+
